<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="idms.model.base.timer.ActiveTask" table="TIMER_ACTIVETASK" >
        <id name="id" type="java.lang.Long" column="ID">
			<generator class="sequence">
				<param name="sequence">S_TIMER</param>
			</generator>
		</id>
		<property name="taskName" type="java.lang.String"></property>
		<property name="startTime" type="java.util.Date"></property>
		<property name="nextTime" type="java.util.Date"></property>
		<property name="lastTime" type="java.util.Date"></property>
		<property name="createTime" type="java.util.Date"></property>
		<property name="lastYxjg" type="java.lang.String"></property>
		<property name="createUser" type="java.lang.String"></property>
		<property name="cycle" type="java.lang.Integer"></property>
		<property name="status" type="java.lang.String"></property>
		<property name="taskDescribe" type="java.lang.String"></property>
		<property name="enable" type="java.lang.Boolean"></property>
		
	</class>
</hibernate-mapping>
