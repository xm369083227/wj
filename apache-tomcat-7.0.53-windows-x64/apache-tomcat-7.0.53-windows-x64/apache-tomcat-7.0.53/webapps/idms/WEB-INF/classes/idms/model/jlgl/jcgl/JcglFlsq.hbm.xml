<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
    <class name="idms.model.jlgl.jcgl.JcglFlsq"  table="JCGL_FLSQ"  >
        <id name="id" type="java.lang.Long">
            <column name="ID" precision="18" scale="0" />
             <generator  class="sequence">
              <param name="S_JCGL_ALL"></param>
            </generator>
        </id>
           <property name="xkfl" type="java.lang.String">
                <column name="XKFL"  length="100" />
            </property>
             <property name="xkflp" type="java.lang.String">
                <column name="XKFLP"  length="100" />
            </property>
            <property name="yhid" type="java.lang.String">
                <column name="YHID" length="20" />
            </property>
            <property name="sqfl" type="java.lang.String">
                <column name="SQFL" length="20" />
            </property>
    </class>
</hibernate-mapping>
